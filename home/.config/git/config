[core]
    compression = 9
    whitespace = error
    preloadindex = true

[init]
    defaultBranch = main

[status]
    branch = true
    showStash = true
    showUntrackedFiles = all

[diff]
    context = 3
    renames = copies
    interhunkcontext = 10

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

